project(kio-ftps)

find_package(KDE4 REQUIRED)

include_directories( ${KDE4_INCLUDES} )

set(mySources ftp.cpp)

kde4_add_plugin( kio-ftps ${mySources} )

target_link_libraries(kio-ftps ${KDE4_KDECORE_LIBS} kio )

install(TARGETS kio-ftps  DESTINATION ${PLUGIN_INSTALL_DIR} )
install( FILES ftps.protocol  DESTINATION  ${SERVICES_INSTALL_DIR} )
